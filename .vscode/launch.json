{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome requests-http on Angular CLI Server",
      "type": "chrome",
      "request": "launch",
      "port": 9292,
      "webRoot": "${workspaceFolder}",
      "url": "http://localhost:4200/#"
    },
    {
      "name": "Docker Node.js Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "platform": "node"
    }
  ]
}